{
  "name": "slidev",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "docs": "npm-run-all -p docs:*",
    "docs:pages": "slidev build pages.md -o docs",
    "docs:slides": "slidev build pages/slides/README.md -o ../../docs/slides",
    "docs:20230716_ai_voice": "slidev build pages/20230716_ai_voice/README.md -o ../../docs/20230716_ai_voice",
    "open:pages": "slidev pages.md --open",
    "open:slides": "slidev pages/slides/README.md --open",
    "open:20230716_ai_voice": "slidev pages/20230716_ai_voice/README.md --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@slidev/cli": "^0.42.5",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "npm-run-all": "^4.1.5"
  }
}
